{
  "expo": {
    "name": "dobbi-app",
    "slug": "dobbi-app",
    "owner": "eeriiclopeezz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": null,
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": null,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": null,
        "backgroundColor": "#ffffff"
      },
      "package": "com.eeriiclopeezz.dobbiapp",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "MEDIA_LIBRARY",
        "MEDIA_LIBRARY_WRITE_ONLY"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": null
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-font",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8a8bf979-f26f-4cb7-80e9-040747f15b11"
      }
    }
  }
}
